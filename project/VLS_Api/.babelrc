    ],
    "plugins": [
        ["@babel/plugin-transform-runtime",
            {
              "regenerator": true
            }
        ],
        [
            "module-resolver",
            {
                "root": ["."],
                "alias": {
                    "@src": "./src"
                }
            }
        ]
    ]
}